    "b = [2,5,33]\n",
    "def mergeShortedList(a,b):\n",
    "    l = []\n",
    "    while a and b:\n",
    "        if a[0] < b[0]:\n",
    "            l.append(a.pop(0))\n",
    "        else:\n",
    "            l.append(b.pop(0))\n",
    "    return l + a + b\n",
    "\n",
